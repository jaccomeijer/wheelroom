{
  "author": "Jacco Meijer",
  "bugs": {
    "url": "https://github.com/jaccomeijer/wheelroom/issues"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "13.4.0",
    "@contentful/rich-text-types": "13.4.0",
    "@emotion/core": "10.0.27",
    "@jacco-meijer/styled-system": "3.0.3",
    "@jacco-meijer/wheelroom": "^3.4.0",
    "@jacco-meijer/wheelroom-plugin-boilerplate": "^2.6.3",
    "@jacco-meijer/wheelroom-plugin-contentful": "^2.9.2",
    "@jacco-meijer/wheelroom-plugin-graphql": "^2.1.16",
    "camelcase": "5.3.1",
    "dotenv": "8.2.0",
    "emotion-theming": "10.0.27",
    "gatsby": "2.18.20",
    "gatsby-plugin-emotion": "4.1.20",
    "gatsby-plugin-manifest": "2.2.37",
    "gatsby-plugin-offline": "3.0.30",
    "gatsby-plugin-react-helmet": "3.1.20",
    "gatsby-plugin-typescript": "2.1.25",
    "gatsby-plugin-web-font-loader": "1.0.4",
    "gatsby-source-contentful": "2.1.72",
    "gatsby-source-filesystem": "2.1.45",
    "gatsby-theme-wheelroom": "^3.0.16",
    "gatsby-transformer-json": "2.2.24",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "yargs": "15.1.0"
  },
  "description": "Gatsby static site boilerplate with React and Typescript",
  "devDependencies": {
    "@types/dotenv": "8.2.0",
    "@types/fs-extra": "8.0.1",
    "@types/node": "13.1.5",
    "@types/react": "16.9.17",
    "@types/react-dom": "16.9.4",
    "@types/react-helmet": "5.0.15",
    "@types/yargs": "15.0.0",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "conventional-changelog-cli": "2.0.31",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.0",
    "fs-extra": "8.1.0",
    "gatsby-plugin-eslint": "2.0.8",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "typescript": "3.7.4"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "homepage": "https://github.com/jaccomeijer/wheelroom#readme",
  "license": "MIT",
  "main": "index.js",
  "name": "boilerplate",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaccomeijer/wheelroom.git"
  },
  "scripts": {
    "lint-fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "build:clean": "rm -rf ./dist",
    "build:gatsby": "gatsby build",
    "netlify": "npm-run-all build:*",
    "compile-config": "tsc --project src/config/tsconfig.json",
    "develop": "gatsby develop",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "start": "npm run develop",
    "type-check": "tsc --noEmit",
    "demo-load-env": "export $(grep -v '^#' .env | xargs)",
    "demo-version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "demo-postversion": "git push && git push --tags",
    "wr": "WHEELROOM_CONFIG=compiled-config/wheelroom-config.js wheelroom",
    "wr:cm": "WHEELROOM_CONFIG=compiled-config/wheelroom-config.js wheelroom create-models",
    "wr:cc": "WHEELROOM_CONFIG=compiled-config/wheelroom-config.js wheelroom create-content boilerplate",
    "wr:dc": "WHEELROOM_CONFIG=compiled-config/wheelroom-config.js wheelroom delete-content boilerplate",
    "wr:cb": "WHEELROOM_CONFIG=compiled-config/wheelroom-config.js wheelroom create-boilerplate react src/components",
    "wr:cg": "WHEELROOM_CONFIG=compiled-config/wheelroom-config.js wheelroom create-graphql gatsbyjs src/components",
    "wr:ls": "WHEELROOM_CONFIG=compiled-config/wheelroom-config.js wheelroom list"
  },
  "type": "commonjs",
  "version": "4.3.8"
}
