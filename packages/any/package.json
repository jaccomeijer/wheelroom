{
  "name": "@wheelroom/any",
  "version": "6.3.32",
  "description": "Any component with CSS element reset and CSS Emotion props",
  "keywords": [
    "any",
    "cli",
    "component",
    "contentful",
    "gatsbyjs",
    "netlify",
    "react",
    "typescript",
    "wheelroom"
  ],
  "homepage": "https://github.com/wheelroom/wheelroom#readme",
  "bugs": {
    "url": "https://github.com/wheelroom/wheelroom/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wheelroom/wheelroom.git"
  },
  "license": "MIT",
  "author": "Wheelroom (https://www.wheelroom.io)",
  "contributors": [
    {
      "name": "Jacco Meijer",
      "email": "jacco@wheelroom.io",
      "url": "https://www.jaccomeijer.nl"
    },
    {
      "name": "Thijs Krooswijk",
      "email": "thijs@wheelroom.io",
      "url": "https://www.thijskrooswijk.com"
    }
  ],
  "type": "module",
  "exports": {
    "./Any": {
      "node": {
        "import": "./build/Any.mjs",
        "require": "./build/Any.cjs"
      },
      "default": "./build/Any.mjs"
    },
    "./exports": {
      "node": {
        "import": "./build/exports.mjs",
        "require": "./build/exports.cjs"
      },
      "default": "./build/exports.mjs"
    },
    "./resets/*": {
      "node": {
        "import": "./build/resets/*.mjs",
        "require": "./build/resets/*.cjs"
      },
      "default": "./build/resets/*.mjs"
    }
  },
  "types": "build/types",
  "scripts": {
    "build": "npm-run-all clean build:*",
    "build:lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "build:module": "rollup -c",
    "build:test": "jest --silent --passWithNoTests",
    "clean": "rimraf build/*",
    "generate": "npm-run-all generate:*",
    "generate:compile": "tsc -p tsconfig.generate.json",
    "generate:update": "node build/generate-elements/generate-elements.js",
    "generate:update-lint-fix": "eslint 'src/elements.tsx' --fix",
    "prepack": "npm run build",
    "release": "standard-version --tag-prefix @wheelroom/any@ --path .",
    "test": "npm run build:test"
  },
  "browserslist": "> 0.25%, not dead",
  "peerDependencies": {
    "@emotion/css": "11.1.3",
    "@emotion/react": "11.1.5",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "engines": {
    "node": ">=15",
    "npm": ">=7.7.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
