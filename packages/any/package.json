{
  "name": "@wheelroom/any",
  "version": "6.24.2",
  "description": "Any component with CSS element reset and CSS Emotion props",
  "files": [
    "CHANGELOG.md",
    "build"
  ],
  "exports": {
    "./any": {
      "import": "./build/any.mjs",
      "require": "./build/any.cjs"
    },
    "./elements": {
      "import": "./build/elements.mjs",
      "require": "./build/elements.cjs"
    },
    "./resets": {
      "import": "./build/resets.mjs",
      "require": "./build/resets.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "build/*"
      ]
    }
  },
  "scripts": {
    "build": "npm-run-all clean build:*",
    "build:lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "build:compile": "rollup --config",
    "build:test": "jest --silent --passWithNoTests",
    "build:types": "tsc -p tsconfig.packages.json --declaration --emitDeclarationOnly --declarationDir build",
    "clean": "rimraf build/*",
    "dev": "rollup --config --watch",
    "generate": "npm-run-all generate:*",
    "generate:compile": "tsc -p tsconfig.generate.json",
    "generate:update": "node build/generate-elements/generate-elements.js",
    "generate:update-lint-fix": "eslint 'src/elements.tsx' --fix"
  },
  "peerDependencies": {
    "@emotion/react": "11.x",
    "react": "17.x",
    "react-dom": "17.x"
  }
}
