{
  "name": "@wheelroom/any",
  "version": "6.3.18",
  "description": "Any component with CSS element reset and CSS Emotion props",
  "keywords": [
    "any",
    "cli",
    "component",
    "contentful",
    "gatsbyjs",
    "netlify",
    "react",
    "typescript",
    "wheelroom"
  ],
  "homepage": "https://github.com/wheelroom/wheelroom#readme",
  "bugs": {
    "url": "https://github.com/wheelroom/wheelroom/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wheelroom/wheelroom.git"
  },
  "license": "MIT",
  "author": "Wheelroom (https://www.wheelroom.io)",
  "contributors": [
    {
      "name": "Jacco Meijer",
      "email": "jacco@wheelroom.io",
      "url": "https://www.jaccomeijer.nl"
    },
    {
      "name": "Thijs Krooswijk",
      "email": "thijs@wheelroom.io",
      "url": "https://www.thijskrooswijk.com"
    }
  ],
  "exports": {
    "./Any": "./build/Any.js",
    "./elements": "./build/elements.js",
    "./resets/*": "./build/resets/*.js"
  },
  "scripts": {
    "build": "npm-run-all clean build:*",
    "build:compile": "webpack --mode production --config webpack/webpack.prod.js",
    "build:lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "build:test": "jest --silent --passWithNoTests",
    "clean": "rimraf build/*",
    "dev": "NODE_ENV=development webpack serve --mode development --port 5000 --hot --config webpack/webpack.dev.js",
    "generate": "npm-run-all generate:*",
    "generate:compile": "tsc -p tsconfig.node10.json",
    "generate:update": "node build/generate-elements/generate-elements.js",
    "generate:update-lint-fix": "eslint 'src/elements.tsx' --fix",
    "prepack": "npm run build",
    "release": "standard-version --tag-prefix @wheelroom/any@ --path packages/any",
    "test": "npm run build:test"
  },
  "engines": {
    "node": ">=15",
    "npm": ">=7.7.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "typesVersions": {
    ">=3.0": {
      ".": [
        "build/elements.d.ts"
      ],
      "any": [
        "build/Any.d.ts"
      ],
      "resets": [
        "resets/resets.d.ts"
      ]
    }
  }
}
