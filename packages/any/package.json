{
  "name": "@wheelroom/any",
  "version": "7.0.0",
  "description": "Any component with CSS element reset and CSS Emotion props",
  "exports": {
    "./Any": "./build/Any.js",
    "./elements": "./build/elements.js",
    "./resets/*": "./build/resets/*.js"
  },
  "scripts": {
    "build": "npm-run-all clean build:*",
    "build:compile": "tsc",
    "build:lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "build:test": "jest --silent --passWithNoTests",
    "clean": "rimraf build/*",
    "generate": "npm-run-all generate:*",
    "generate:compile": "tsc",
    "generate:lint-fix": "eslint 'src/elements.tsx' --fix",
    "generate:update": "node build/generate-elements/generate-elements.js",
    "release": "standard-version -t \"@wheelroom/any@\"",
    "release:dry-run": "npm run release -- --dry-run",
    "version:patch": "npm run release -- --release-as patch",
    "version:minor": "npm run release -- --release-as minor",
    "version:major": "npm run release -- --release-as major",
    "prepack": "npm run build",
    "test": "npm run build:test"
  },
  "typesVersions": {
    ">=3.0": {
      ".": [
        "build/elements.d.ts"
      ],
      "any": [
        "build/Any.d.ts"
      ],
      "resets": [
        "resets/resets.d.ts"
      ]
    }
  }
}
