    "contentful",
    "netlify",
    "cli",
    "react",
    "typescript"
  ],
  "devDependencies": {
    "@svgr/cli": "5.2.0",
    "@types/dotenv": "8.2.0",
    "@types/fs-extra": "8.1.0",
    "@types/node": "13.7.7",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/react-helmet": "5.0.15",
    "@types/yargs": "15.0.4",
    "@typescript-eslint/eslint-plugin": "2.22.0",
    "@typescript-eslint/parser": "2.22.0",
    "conventional-changelog-cli": "2.0.31",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.5.0",
    "fs-extra": "8.1.0",
    "gatsby-plugin-eslint": "2.0.8",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "typescript": "3.8.3"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "homepage": "https://github.com/wheelroom/wheelroom-io#readme",
  "license": "MIT",
  "main": "index.js",
  "name": "wheelroom-io",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wheelroom/wheelroom-io.git"
  },
  "scripts": {
    "lint-fix": "eslint 'src/**/*.{js,ts,tsx}' --fix",
    "build:clean": "rm -rf ./dist",
    "build:gatsby": "gatsby build",
    "netlify": "npm-run-all build:*",
    "compile-config": "tsc --project src/config/tsconfig.json",
    "develop": "gatsby develop",
    "lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "start": "npm run develop",
    "svgr": "svgr --filename-case kebab --ext tsx --no-dimensions --out-dir src/svg src/svg",
    "type-check": "tsc --noEmit",
    "demo-load-env": "export $(grep -v '^#' .env | xargs)",
    "demo-version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "demo-postversion": "git push && git push --tags",
    "wr": "WHEELROOM_CONFIG=compiled-config/wheelroom-config.js wheelroom",
    "wr:cm": "WHEELROOM_CONFIG=compiled-config/wheelroom-config.js wheelroom create-models",
    "wr:cc": "WHEELROOM_CONFIG=compiled-config/wheelroom-config.js wheelroom create-content boilerplate",
    "wr:dc": "WHEELROOM_CONFIG=compiled-config/wheelroom-config.js wheelroom delete-content boilerplate",
    "wr:cb": "WHEELROOM_CONFIG=compiled-config/wheelroom-config.js wheelroom create-boilerplate react src/components",
    "wr:cg": "WHEELROOM_CONFIG=compiled-config/wheelroom-config.js wheelroom create-graphql gatsbyjs src/components",
    "wr:ls": "WHEELROOM_CONFIG=compiled-config/wheelroom-config.js wheelroom list"
  },
  "type": "commonjs",
  "version": "1.0.0"
}
