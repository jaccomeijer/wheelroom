    "@jacco-meijer/wheelroom-plugin-contentful": "^2.11.0",
    "@jacco-meijer/wheelroom-plugin-graphql": "^2.2.0",
    "@reach/router": "1.3.3",
    "camelcase": "5.3.1",
    "dotenv": "8.2.0",
    "emotion-theming": "10.0.27",
    "gatsby": "2.19.27",
    "gatsby-plugin-emotion": "4.1.23",
    "gatsby-plugin-manifest": "2.2.42",
    "gatsby-plugin-offline": "3.0.35",
    "gatsby-plugin-react-helmet": "3.1.22",
