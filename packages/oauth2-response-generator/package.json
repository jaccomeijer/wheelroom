{
  "name": "@wheelroom/oauth2-response-generator",
  "version": "1.0.0",
  "description": "Oauth 2.0 server",
  "keywords": [
    "any",
    "cli",
    "component",
    "contentful",
    "gatsbyjs",
    "netlify",
    "react",
    "typescript",
    "wheelroom"
  ],
  "homepage": "https://github.com/wheelroom/wheelroom#readme",
  "bugs": {
    "url": "https://github.com/wheelroom/wheelroom/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wheelroom/wheelroom.git"
  },
  "license": "MIT",
  "author": "Wheelroom (https://www.wheelroom.io)",
  "contributors": [
    {
      "name": "Jacco Meijer",
      "email": "jacco@wheelroom.io",
      "url": "https://www.jaccomeijer.nl"
    },
    {
      "name": "Thijs Krooswijk",
      "email": "thijs@wheelroom.io",
      "url": "https://www.thijskrooswijk.com"
    }
  ],
  "exports": {
    "./plain": {
      "import": "./build/plain.mjs",
      "require": "./build/plain.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "build/*"
      ]
    }
  },
  "files": [
    "CHANGELOG.md",
    "build"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm-run-all clean build:* copy-templates",
    "build:compile": "rollup --config",
    "build:lint": "eslint 'src/**/*.{js,ts,tsx}'",
    "build:test": "jest --silent --passWithNoTests",
    "build:types": "tsc -p tsconfig.packages.json --declaration --emitDeclarationOnly --declarationDir build",
    "clean": "rimraf build/*",
    "dev": "rollup --config --watch"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "5.0.0-alpha.8",
    "jsonwebtoken": "^8.5.1",
    "jsrsasign": "10.1.12",
    "ms": "^2.1.3",
    "supertest": "^6.1.3",
    "uri-js": "^4.4.1",
    "uuid": "3.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^14.14.31",
    "@types/supertest": "^2.0.10"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.11.1"
  }
}
