  "devDependencies": {
    "@types/jest": "24.0.25",
    "@types/node": "13.1.5",
    "@types/react-dom": "16.9.4",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "eslint": "6.8.0",
