{
  "name": "@wheelroom/gatsby-starter",
  "version": "6.4.4",
  "description": "A JAMstack boilerplate for services, like Gatsby, Contentful, Netlify and much more",
  "scripts": {
    "build": "npm-run-all clean build:*",
    "build:compile": "gatsby build",
    "build:lint": "eslint 'src/**/*.{ts,tsx}'",
    "build:test": "jest --silent --passWithNoTests",
    "clean": "gatsby clean",
    "dev": "gatsby develop",
    "release": "standard-version",
    "serve": "gatsby serve"
  },
  "dependencies": {
    "@wheelroom/any": "6.3.27",
    "gatsby": "3.1.2"
  },
  "keywords": [
    "any",
    "cli",
    "component",
    "contentful",
    "gatsbyjs",
    "netlify",
    "react",
    "typescript",
    "wheelroom"
  ],
  "homepage": "https://github.com/wheelroom/wheelroom#readme",
  "bugs": {
    "url": "https://github.com/wheelroom/wheelroom/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wheelroom/wheelroom.git"
  },
  "license": "MIT",
  "author": "Wheelroom (https://www.wheelroom.io)",
  "contributors": [
    {
      "name": "Jacco Meijer",
      "email": "jacco@wheelroom.io",
      "url": "https://www.jaccomeijer.nl"
    },
    {
      "name": "Thijs Krooswijk",
      "email": "thijs@wheelroom.io",
      "url": "https://www.thijskrooswijk.com"
    }
  ],
  "engines": {
    "node": ">=15",
    "npm": ">=7.7.6"
  },
  "publishConfig": {
    "access": "public"
  }
}
